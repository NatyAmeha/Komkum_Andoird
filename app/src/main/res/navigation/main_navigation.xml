        android:id="@+id/bookHomeFragment"
        android:name="com.zomatunes.zomatunes.ui.book.BookHomeFragment"
        android:label="BookHomeFragment" />
    <fragment
        android:id="@+id/libraryFragment"
        android:name="com.zomatunes.zomatunes.ui.library.LibraryFragment"
        android:label="LibraryFragment" />
    <fragment
        android:id="@+id/queueListFragment"
        android:name="com.zomatunes.zomatunes.ui.player.QueueListFragment"
        android:label="QueueListFragment" />
    <fragment
        android:id="@+id/userFragment"
        android:name="com.zomatunes.zomatunes.ui.user.UserFragment"
        android:label="user_fragment"
        tools:layout="@layout/user_fragment" />
