<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/baseFragment"
        android:name="com.zomatunes.zomatunes.ui.home.BaseFragment"
        android:label="fragment_base"
        tools:layout="@layout/fragment_base" >
        <action
            android:id="@+id/action_baseFragment_to_albumFragment"
            app:destination="@id/albumFragment" />
        <action
            android:id="@+id/action_baseFragment_to_albumListFragment"
            app:destination="@id/albumListFragment" />
        <action
            android:id="@+id/action_baseFragment_to_playlistFragment"
            app:destination="@id/playlistFragment" />
        <action
            android:id="@+id/action_baseFragment_to_artistFragment"
            app:destination="@id/artistFragment" />
        <action
            android:id="@+id/action_baseFragment_to_songListFragment"
            app:destination="@id/songListFragment" />
    </fragment>
    <fragment
        android:id="@+id/albumFragment"
        android:name="com.zomatunes.zomatunes.ui.album.AlbumFragment"
        android:label="AlbumFragment" >
        <action
            android:id="@+id/action_albumFragment_to_artistFragment"
            app:destination="@id/artistFragment" />
    </fragment>
    <fragment
        android:id="@+id/albumListFragment"
        android:name="com.zomatunes.zomatunes.ui.album.AlbumListFragment"
        android:label="AlbumListFragment" >
        <action
            android:id="@+id/action_albumListFragment_to_albumFragment"
            app:destination="@id/albumFragment" />
    </fragment>
    <fragment
        android:id="@+id/playlistFragment"
        android:name="com.zomatunes.zomatunes.ui.playlist.PlaylistFragment"
        android:label="PlaylistFragment" />
    <fragment
        android:id="@+id/artistFragment"
        android:name="com.zomatunes.zomatunes.ui.artist.ArtistFragment"
        android:label="ArtistFragment" >
        <action
            android:id="@+id/action_artistFragment_to_albumFragment"
            app:destination="@id/albumFragment" />
    </fragment>
    <fragment
        android:id="@+id/songListFragment"
        android:name="com.zomatunes.zomatunes.ui.song.SongListFragment"
        android:label="SongListFragment" />
    <fragment
        android:id="@+id/addToFragment"
        android:name="com.zomatunes.zomatunes.ui.home.AddToFragment"
        android:label="AddToFragment" />
    <fragment
        android:id="@+id/artistListFragment"
        android:name="com.zomatunes.zomatunes.ui.artist.ArtistListFragment"
        android:label="ArtistListFragment" >
