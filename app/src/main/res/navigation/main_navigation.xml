    <fragment
        android:id="@+id/bookCollectionFragment"
        android:name="com.zomatunes.zomatunes.ui.book.BookCollectionFragment"
        android:label="BookCollectionFragment" />
    <fragment
        android:id="@+id/bookListFragment"
        android:name="com.zomatunes.zomatunes.ui.book.BookListFragment"
        android:label="BookListFragment" />
</navigation>
