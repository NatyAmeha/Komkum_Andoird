<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/baseFragment"
        android:name="com.example.ethiomusic.ui.home.BaseFragment"
        android:label="fragment_base"
        tools:layout="@layout/fragment_base" >
        <action
            android:id="@+id/action_baseFragment_to_albumFragment"
            app:destination="@id/albumFragment" />
        <action
            android:id="@+id/action_baseFragment_to_albumListFragment"
            app:destination="@id/albumListFragment" />
        <action
            android:id="@+id/action_baseFragment_to_playlistFragment"
            app:destination="@id/playlistFragment" />
        <action
            android:id="@+id/action_baseFragment_to_artistFragment"
            app:destination="@id/artistFragment" />
        <action
            android:id="@+id/action_baseFragment_to_songListFragment"
            app:destination="@id/songListFragment" />
    </fragment>
    <fragment
        android:id="@+id/albumFragment"
        android:name="com.example.ethiomusic.ui.album.AlbumFragment"
        android:label="AlbumFragment" >
        <action
            android:id="@+id/action_albumFragment_to_artistFragment"
            app:destination="@id/artistFragment" />
    </fragment>
    <fragment
        android:id="@+id/albumListFragment"
        android:name="com.example.ethiomusic.ui.album.AlbumListFragment"
        android:label="AlbumListFragment" >
        <action
            android:id="@+id/action_albumListFragment_to_albumFragment"
            app:destination="@id/albumFragment" />
    </fragment>
    <fragment
        android:id="@+id/playlistFragment"
        android:name="com.example.ethiomusic.ui.playlist.PlaylistFragment"
        android:label="PlaylistFragment" />
    <fragment
        android:id="@+id/artistFragment"
        android:name="com.example.ethiomusic.ui.artist.ArtistFragment"
        android:label="ArtistFragment" >
        <action
            android:id="@+id/action_artistFragment_to_albumFragment"
            app:destination="@id/albumFragment" />
    </fragment>
    <fragment
        android:id="@+id/songListFragment"
        android:name="com.example.ethiomusic.ui.song.SongListFragment"
        android:label="SongListFragment" />
    <fragment
        android:id="@+id/addToFragment"
        android:name="com.example.ethiomusic.ui.home.AddToFragment"
        android:label="AddToFragment" />
    <fragment
        android:id="@+id/artistListFragment"
        android:name="com.example.ethiomusic.ui.artist.ArtistListFragment"
        android:label="ArtistListFragment" >
        <action
            android:id="@+id/action_artistListFragment_to_artisSelectiontListFragment2"
            app:destination="@id/artisSelectiontListFragment2" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.ethiomusic.ui.search.SearchFragment"
        android:label="SearchFragment" />
    <fragment
        android:id="@+id/downloadListFragment"
        android:name="com.example.ethiomusic.ui.download.DownloadListFragment"
        android:label="DownloadListFragment" />
    <fragment
        android:id="@+id/radioFragment"
        android:name="com.example.ethiomusic.ui.radio.RadioFragment"
        android:label="radio_fragment"
        tools:layout="@layout/radio_fragment" />
    <fragment
        android:id="@+id/radioHomeFragment"
        android:name="com.example.ethiomusic.ui.radio.RadioHomeFragment"
        android:label="RadioHomeFragment" />
    <fragment
        android:id="@+id/artistInfoFragment"
        android:name="com.example.ethiomusic.ui.artist.ArtistInfoFragment"
        android:label="ArtistInfoFragment" />
    <fragment
        android:id="@+id/addSongFragment"
        android:name="com.example.ethiomusic.ui.song.AddSongFragment"
        android:label="AddSongFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.ethiomusic.ui.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/artistsHomeFragment"
        android:name="com.example.ethiomusic.ui.artist.ArtistsHomeFragment"
        android:label="ArtistsHomeFragment" />
    <fragment
        android:id="@+id/libraryFragment"
        android:name="com.example.ethiomusic.ui.library.LibraryFragment"
        android:label="LibraryFragment" />
    <fragment
        android:id="@+id/queueListFragment"
        android:name="com.example.ethiomusic.ui.player.QueueListFragment"
        android:label="QueueListFragment" />
    <fragment
        android:id="@+id/userFragment"
        android:name="com.example.ethiomusic.ui.user.UserFragment"
        android:label="user_fragment"
        tools:layout="@layout/user_fragment" />
    <fragment
        android:id="@+id/playerFragment"
        android:name="com.example.ethiomusic.ui.player.PlayerFragment"
        android:label="PlayerFragment" />
    <fragment
        android:id="@+id/browseMusicFragment"
        android:name="com.example.ethiomusic.ui.browse.BrowseMusicFragment"
        android:label="BrowseMusicFragment" />
    <fragment
        android:id="@+id/artisSelectiontListFragment2"
        android:name="com.example.ethiomusic.ui.onboarding.ArtisSelectiontListFragment"
        android:label="ArtisSelectiontListFragment" />
</navigation>
