        android:id="@+id/addSongFragment"
        android:name="com.zomatunes.zomatunes.ui.song.AddSongFragment"
        android:label="AddSongFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.zomatunes.zomatunes.ui.home.HomeFragment"
        android:label="HomeFragment" />
