        android:id="@+id/bookBrowseFragment"
        android:name="com.zomatunes.zomatunes.ui.book.BookBrowseFragment"
        android:label="BookBrowseFragment" >
        <action
            android:id="@+id/action_bookBrowseFragment_to_bookSellFragment"
            app:destination="@id/audiobookSellFragment"
            app:popUpTo="@id/bookBrowseFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/reviewListFragment"
        android:name="com.zomatunes.zomatunes.ui.book.ReviewListFragment"
        android:label="fragment_review_list"
        tools:layout="@layout/fragment_review_list" />
    <fragment
        android:id="@+id/subscriptionFragment2"
        android:name="com.zomatunes.zomatunes.ui.account.subscription.SubscriptionFragment"
        android:label="SubscriptionFragment" />
    <fragment
        android:id="@+id/friendsListFragment"
        android:name="com.zomatunes.zomatunes.ui.user.FriendsListFragment"
        android:label="FriendsListFragment" />
    <fragment
        android:id="@+id/friendActivityFragment"
        android:name="com.zomatunes.zomatunes.ui.user.FriendActivityFragment"
        android:label="FriendActivityFragment" />
    <fragment
        android:id="@+id/authorFragment"
        android:name="com.zomatunes.zomatunes.ui.author.AuthorFragment"
        android:label="AuthorFragment" />
    <fragment
        android:id="@+id/EBookSellFragment"
        android:name="com.zomatunes.zomatunes.ui.book.EBookSellFragment"
        android:label="EBookSellFragment" />
    <fragment
        android:id="@+id/bookReaderFragment"
        android:name="com.zomatunes.zomatunes.ui.book.BookReaderFragment"
        android:label="fragment_book_reader"
        tools:layout="@layout/fragment_book_reader" />
    <fragment
        android:id="@+id/fullscreenFragment"
        android:name="com.zomatunes.zomatunes.FullscreenFragment"
        android:label="fragment_fullscreen"
