        android:name="com.zomatunes.zomatunes.ui.book.AudiobookSellFragment"
        android:label="BookSellFragment">
        <deepLink app:uri="www.example.com/user" />
    </fragment>
    <fragment
        android:id="@+id/audiobookFragment"
        android:name="com.zomatunes.zomatunes.ui.book.AudiobookFragment"
        android:label="BookFragment">

    </fragment>
